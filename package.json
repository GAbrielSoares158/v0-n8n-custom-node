{
  "name": "n8n-nodes-random-generator",
  "version": "1.0.0",
  "description": "Nó n8n personalizado para gerar números aleatórios verdadeiros usando a API Random.org",
  "keywords": [
    "n8n-community-node-package"
  ],
  "license": "MIT",
  "homepage": "https://github.com/gabrielsoaresreis2004/onfly-n8n-challenge",
  "author": {
    "name": "Gabriel Soares",
    "email": "gabrielsoaresreis2004@gmail.com"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/gabrielsoaresreis2004/onfly-n8n-challenge.git"
  },
  "engines": {
    "node": ">=22.0.0"
  },
  "packageManager": "npm@10.0.0",
  "main": "index.js",
  "scripts": {
    "build": "tsc && npm run copy-icons",
    "copy-icons": "cp -r src/nodes/Random/icon.svg dist/nodes/Random/",
    "dev": "tsc --watch",
    "format": "prettier --write .",
    "lint": "eslint src --ext .ts",
    "lintfix": "eslint src --ext .ts --fix",
    "prepublishOnly": "npm run build && npm run lint",
    "test": "jest"
  },
  "files": [
    "dist"
  ],
  "n8n": {
    "n8nNodesApiVersion": 1,
    "nodes": [
      "dist/nodes/Random/Random.node.js"
    ]
  },
  "devDependencies": {
    "@types/jest": "^29.5.12",
    "@types/node": "^22.0.0",
    "@typescript-eslint/eslint-plugin": "^7.0.0",
    "@typescript-eslint/parser": "^7.0.0",
    "eslint": "^8.57.0",
    "eslint-plugin-n8n-nodes-base": "^1.16.1",
    "jest": "^29.7.0",
    "prettier": "^3.2.5",
    "ts-jest": "^29.1.2",
    "typescript": "^5.4.5",
    "@types/react": "19.1.13",
    "@types/react-dom": "19.1.9",
    "tailwindcss": "4.1.13",
    "postcss": "8.5.6"
  },
  "peerDependencies": {
    "n8n-workflow": "*"
  },
  "dependencies": {
    "axios": "^1.7.7",
    "n8n-workflow": "1.110.0",
    "next": "15.5.4",
    "react": "19.1.1",
    "react-dom": "19.1.1",
    "class-variance-authority": "0.7.1",
    "clsx": "2.1.1",
    "lucide-react": "0.544.0",
    "tailwind-merge": "3.3.1",
    "tailwindcss-animate": "1.0.7"
  }
}
